{new
   GHScreen
   practice_change_speed_screen
   (panels PRACTICE_PANELS sel_speed_panel helpbar)
   (focus sel_speed_panel)
   (animate_transition FALSE)
   (in_game TRUE)
   (back_screen
      "")
   (helpbar
      (default
         ((fret1 help_continue)
            (fret2 help_back)
            (strum help_updown))))
   (enter
      {mtv_overlay_panel set_showing FALSE})
   (SELECT_START_MSG
      {game_restart}
      kDataUnhandled)
   (BUTTON_DOWN_MSG
      {if
         {== $button kPad_Tri}
         {ui
            goto_screen
            [back_screen]}}
      kDataUnhandled)}
