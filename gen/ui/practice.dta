{new
   GHPanel
   practice_panel
   (file practice_panel.milo)
   (enter
      {events_parser add_sink $this section}
      {game add_sink $this intro_skip}
      {foreach
         $sfx
         (practice_snare practice_kick practice_hat)
         {$sfx add_fader music_fader}})
   (intro_skip
      {mtv_overlay_panel show_overlay TRUE}
      {script_task
         (delay 3)
         (units kTaskSeconds)
         (script
            {mtv_overlay_panel show_overlay FALSE})}
      {set
         [ignore_section]
         {section_provider section_after_last}})
   (update
      {click_parser remove_sink $this}
      {if
         {&&
            {!=
               {gamecfg get practice_speed}
               0}
            {! $stretch_bg}}
         {click_parser add_sink $this}}
      {beatmatch update_practice_speed}
      {if
         {!
            [keep_section_text]}
         {section.lbl set_text ''}}
      {set
         [keep_section_text]
         FALSE})
   (exit
      {click_parser remove_sink $this}
      {events_parser remove_sink $this}
      {game remove_sink $this})
   (midi_parser
      ($msg)
      {$this $msg})
   (click_snare
      {practice_snare play})
   (click_kick
      {practice_kick play})
   (click_hat
      {practice_hat play})
   (section
      ($sec)
      {section.lbl
         set_text
         {if_else
            {==
               $sec
               [ignore_section]}
            ''
            $sec}})
   (keep_section_text FALSE)
   (ignore_section '')}
#define PRACTICE_PANELS
(practice_panel game hud track_panel mtv_overlay_panel)
{new
   GHScreen
   practice_game_screen
   (panels PRACTICE_PANELS)
   (allow_back FALSE)
   (animate_transition FALSE)
   (in_game TRUE)
   (BUTTON_DOWN_MSG
      {switch
         $button
         (kPad_Select
            {game_restart_fast})
         (kPad_Start
            {if {! {game get game_over}}
               {game set_paused TRUE}
               {ui goto_screen pract_pause_screen}})})
      ; {if
      ;    {&&
      ;       {== $button kPad_Start}
      ;       {!
      ;          {game get game_over}}}
      ;    {$this pause_practice}})
   (pause_practice
      {game set_paused TRUE}
      {ui goto_screen pract_pause_screen})
   (enter
      {practice_panel update})
   (TRANSITION_COMPLETE_MSG
      {if
         $game_unpause_on_enter
         {game set_paused FALSE}
         {set $game_unpause_on_enter FALSE}})
   (poll
      {if
         {game is_missing_controller}
         {game set_paused TRUE}
         {ui goto_screen practice_pause_controller_screen}}
      {if
         {&&
            {profilemgr guide_showing}
            {!
               {game paused}}
            {!
               {game get game_over}}
            {!
               {ui in_transition}}}
         {$this pause_practice}})}
{new
   GHPanel
   practice_end_panel
   (file practice_end.milo)
   (enter
      {$this set_focus restart.btn}
      {do
         ($pat
            {localize gems_hit_pattern})
         ($hits
            {player0 gems_hit})
         ($tot
            {'+'
               $hits
               {player0 gems_passed}})
         ($perc
            {int
               {'*'
                  100
                  {/ $hits $tot}}})
         {stats.lbl
            set_localized_text
            {sprintf $pat $perc $hits $tot}}}
      {do
         ($career
            {practice_sel_section_screen get career_mode})
         {pause_buttons_career.view set_showing $career}
         {pause_buttons.view
            set_showing
            {! $career}}
         {$this
            set_focus
            {if_else $career restart_career.btn restart.btn}}}
      {if
         {campaign
            check_practice_achievement
            {gamecfg get_song}}
         {gamecfg set allow_achievements TRUE}
         {achievements submit practice}
         {gamecfg set allow_achievements FALSE}})
   (SELECT_START_MSG
      {switch
         $component
         (restart.btn
            {game_restart})
         (restart_career.btn
            {game_restart})
         (change_speed.btn
            {practice_change_speed_screen set back_screen practice_end_screen}
            {ui goto_screen practice_change_speed_screen})
         (change_speed_career.btn
            {practice_change_speed_screen set back_screen practice_end_screen}
            {ui goto_screen practice_change_speed_screen})
         (new_sec.btn
            {meta_loading_nosave_goto practice_sel_section_screen})
         (new_sec_career.btn
            {meta_loading_goto practice_sel_section_screen})
         (new_song.btn
            {meta_loading_nosave_goto practice_selsong_screen})
         (quit.btn
            {meta_loading_nosave_goto main_screen})
         (quit_career.btn
            {career_screen resume_from_practice}
            {meta_loading_goto career_screen})})}
{new
   GHScreen
   practice_end_screen
   (panels PRACTICE_PANELS practice_end_panel helpbar)
