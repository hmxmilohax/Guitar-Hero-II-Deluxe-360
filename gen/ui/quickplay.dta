               {character_provider num_outfits $sel_char}
               1}
            {sel_character_panel_qp choose_skins}
            {do
               {game
                  set_character
                  {sel_character_panel_qp get char_outfit}
                  TRUE}
               {ui
                  goto_screen
                  [done_screen]}}}}})
