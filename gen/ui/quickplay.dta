#define SEL_CHARACTER_SELECT_HANDLER_QP
({if_else
      {sel_character_panel_qp get skin_select}
      {do
         ($sel_outfit
            {sel_character_panel_qp get char_outfit})
         {if
            {&&
               $sel_outfit
               {!=
                  {game get_character_outfit}
                  $sel_outfit}}
            {game set_character $sel_outfit}}
         {ui
            goto_screen
            [done_screen]}}
      {do
         ($sel_char
